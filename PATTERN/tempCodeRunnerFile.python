# Define the number of rows and columns
rows = 5
columns = 3

# Loop over each row
for row in range(rows):
    # Loop over each column
    for col in range(columns):
        # Determine whether to print an asterisk or not
        if (row < 3 and col == 0) or (row < 2 and col < 2) or (row == 2 and col < 3):
            print("*", end="")
        else:
            print(" ", end="")  # Print a space for alignment
    print()  # Move to the next line